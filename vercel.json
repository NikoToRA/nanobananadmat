{
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node"
    },
    "server.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/generate",
      "destination": "/api/generate"
    },
    {
      "source": "/api/generate-from-image",
      "destination": "/api/generate-from-image"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}
